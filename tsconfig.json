{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"allowJs": true,
		"jsx": "react",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": false,
		"preserveConstEnums": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"baseUrl": ".",
		"lib": ["dom", "es2015", "es2016"]
	}
}
